{
  "manifest_version": 2,

  "name": "GistFiddle",
  "description": "Opens a GitHub Gist with JSFiddle",

  "version": "1.0",

  "permissions": [
    "https://gist.github.com/",
    "http://jsfiddle.net/"
  ],

  "content_scripts": [
    {
      "matches": ["https://gist.github.com/*"],
      "js": ["jquery.js", "script.js"],
      "css": ["fontello/css/fontello-embedded.css"]
    }
  ]
}